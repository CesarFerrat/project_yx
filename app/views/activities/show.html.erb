  <div class="col-md-2">
  </div>
  <div class="col-md-8">
      <% @activities.each do |activity| %>
          <div class="activity">
            <div class="row">
              <div class='col-sm-9'/>
                <%= render_activity activity %>
              </div>
              <div class='col-sm-3 pull-right'/>
                <small class="datetime"><%= activity.created_at.strftime('%-m/%e %l:%M%P') %></small>
              </div>
            </div>
        </div>
      <% end %>
  </div>
  <div class="col-md-2">
  </div>
