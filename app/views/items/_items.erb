<% @items.each do |item| %>
  <div class="galleryItem">
    <a><%= image_tag item.picture.url(:medium)%></a>
    <strong><%= item.name %></strong>
    <li class="dropdown" style="list-style-type: none;">
      <a href="#" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-trash pull-right" style="color:red"></span></a>
        <ul class="dropdown-menu">
          <li><%= link_to 'Delete',url_for(action: :destroy,id: item.id),method: :delete, data: {confirm: "Are you sure?"} %></li>
        </ul>
    </li>
  </div>
<% end %>
